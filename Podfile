source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

target 'Marvel' do
    pod 'Alamofire', '~> 3.3'
    pod 'SwiftyJSON', '~> 2.3'
    pod 'SnapKit'
    pod 'Kingfisher'
    
    target 'MarvelTests' do
        inherit! :search_paths
        pod 'Quick'
        pod 'Nimble'
    end
end

