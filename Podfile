source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

target 'Marvel' do
    pod 'Alamofire', '~> 4.0'
    pod 'SwiftyJSON', '~> 3.0'
    pod 'SnapKit', '~> 3.0'
    pod 'Kingfisher', '~> 3.0'
    pod 'SVProgressHUD', '~> 2.0'
    pod 'DZNEmptyDataSet', '~> 1.8'

    target 'MarvelTests' do
        inherit! :search_paths
        pod 'Quick'
        pod 'Nimble', '~> 5.0'
    end
end

